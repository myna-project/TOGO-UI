{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "togo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/togo",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              { "glob": "**/*", "input": "src/assets/", "output": "/assets/" },
              { "glob": "**/*", "input": "./node_modules/d3-mitch-tree/dist", "output": "/assets/d3-mitch-tree" }
            ],
            "styles": [
              "src/custom-theme.scss",
              "src/styles.scss",
              "./node_modules/d3-mitch-tree/dist/css/d3-mitch-tree.css",
              "./node_modules/d3-mitch-tree/dist/css/d3-mitch-tree-theme-default.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules"
              ]
            },
            "scripts": [
              "./node_modules/d3-mitch-tree/dist/js/d3-mitch-tree.js"
            ],
            "allowedCommonJsDependencies": [
              "buffer", "highcharts", "highcharts-angular"
            ]
          },
          "configurations": {}
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "togo:build"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "togo:build"
          }
        }
      }
    }
  },
  "defaultProject": "togo",
  "cli": {
    "analytics": false
  }
}
